   "source": [
    "#Q1 请用Python编写冒泡排序的算法\n",
    "arr=[7,9,0,-1,6,4,3,1,2,5]\n",
    "l=len(arr)\n",
    "for i in range(l):\n",
    "    for j in range(0,l-1):\n",
    "        if arr[j]>arr[j+1]:\n",
    "            arr[j], arr[j+1]=arr[j+1], arr[j]\n",
    "    l=l-1\n",
    "print(arr)"
   ]
  }
